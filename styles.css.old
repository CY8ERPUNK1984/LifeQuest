:root {
    --primary-color: #4CAF50;
    --secondary-color: #FF9800;
    --tertiary-color: #FFC107;
    --background-color: #f5f5f5;
    --card-bg-color: #ffffff;
    --text-color: #333333;
    --light-text: #777777;
    --border-radius: 16px;
    --shadow: 0 4px 12px rgba(0, 0, 0, 0.1);
}

/* Новые стили для аудио-плеера */
.audio-controls-main {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 10px;
    flex-wrap: wrap;
}

.audio-track-selector {
    min-width: 120px;
    margin-left: 10px;
}

.audio-select {
    width: 100%;
    padding: 8px 10px;
    border-radius: 8px;
    border: 1px solid #ddd;
    background-color: white;
    font-size: 14px;
    outline: none;
    cursor: pointer;
}

.audio-player-header-fixed h3 {
    font-size: 16px;
    font-weight: 500;
    margin: 0;
    color: var(--text-color);
}

/* Адаптивные стили для аудио-плеера */
@media (max-width: 480px) {
    .audio-player-header-fixed {
        padding: 10px;
    }

    .audio-controls-main {
        flex-direction: column;
        align-items: stretch;
    }

    .audio-track-selector {
        margin-left: 0;
        margin-top: 10px;
    }
}

